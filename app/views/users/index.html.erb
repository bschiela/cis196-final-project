<p id="notice"><%= notice %></p>

<div class="container">
	<h2>Artists</h2>

	<table>
  	<thead>
    	<tr>
      	<th colspan="3"></th>
    	</tr>
  	</thead>

  	<tbody>
    	<% @users.each do |user| %>
				<tr>
					<td><%= image_tag user.image.url(:thumb) %></td>
					<td><%= link_to "#{user.name}", user %></td>
					<td><%= user.bio %></td>
      	</tr>
    	<% end %>
  	</tbody>
	</table>
</div>
